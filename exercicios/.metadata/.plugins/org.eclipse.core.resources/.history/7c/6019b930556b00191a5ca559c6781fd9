package br.com.fiap.beans;

public class Medio extends Formacao {
	private String tipo;

	public Medio(String descncao, int periodo, double mensalidade, int duracao, String tipo) {
		super(descncao, periodo, mensalidade, duracao);
		this.tipo = tipo;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
}
